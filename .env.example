# DB (SQLite for MVP)
DB_DB_URL=sqlite:///./data/app.db

# Qdrant (docker-compose or local)
QDRANT_URL=http://localhost:6333

# LLM - Ollama only for MVP
LLM_OLLAMA_ENABLED=true
LLM_OLLAMA_API_BASE=http://localhost:11434
LLM_OLLAMA_MODEL=ollama/llama3.2
LLM_GEMINI_ENABLED=false

# Embeddings - Ollama (orchestrator uses this)
EMBED_OLLAMA_API_BASE=http://localhost:11434
EMBED_OLLAMA_MODEL=nomic-embed-text
EMBED_DIMS=768
